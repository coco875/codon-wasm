from time import time

def fib(n):
    return n if n < 2 else fib(n - 1) + fib(n - 2)

@export
def fn() -> None:
    t0 = time()
    ans = fib(39)
    try:
        for i in range(10):
            # if i >= 2:
            #     continue
            a = [0,1][i]
            print("a:", a)
            if a != 0:
                print("1/a:", 1/a)
    except:
        print("error")
    t1 = time()
    print(f'Computed fib(40) = {ans} in {t1 - t0} seconds.')

fn()